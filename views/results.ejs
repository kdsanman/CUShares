      <div class="mpd3">
         <%= tweets.length %>
        <% for(var i=0; i<tweets.length; i++) {%>
          <blockquote class="twitter-tweet" lang="en"><p lang="en" dir="ltr"> <%= tweets[i].text %> <a href="https://twitter.com/<%= tweets[i].user.screen_name %>/status/<%= tweets[i].id_str %>"></a></blockquote> 
